{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CLIYoutube",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/CLIYoutube",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "preLaunchTask": "Build CLIYoutube with CMake",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
